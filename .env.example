# Mobile Message API Credentials
# Copy this file to .env and add your real credentials

API_USERNAME=your_api_username_here
API_PASSWORD=your_api_password_here

# Test phone number (where SMS will be sent)
TEST_PHONE_NUMBER=04000000000

# Sender phone number (your registered sender number)
SENDER_PHONE_NUMBER=your_sender_phone_here

# Set to true to enable real SMS sending in integration tests
# WARNING: This will send actual SMS and use credits from your account
ENABLE_REAL_SMS_TESTS=false

# Set to false to disable bulk SMS tests (recommended to avoid high costs)
ENABLE_BULK_SMS_TESTS=false 